#  Fonts
font_size 9.0
# url_color #0087bd
url_style curly
detect_urls yes
show_hyperlink_targets yes

#  style
background_opacity 0.8

#  keybindings
map ctrl+v paste_from_clipboard
map ctrl+c copy_to_clipboard


# BEGIN_KITTY_THEME
# Base2Tone Drawbridge Dark
include current-theme.conf
# END_KITTY_THEME

sync_to_monitor no
